{
  "description": "KiCad - A Cross Platform and Open Source Electronics Design Automation Suite.",
  "docker_registry": "https://ghcr.io/",
  "image_src": "kicad.png",
  "run_config": {
    "security_opt": [
      "seccomp=unconfined"
    ],
    "entrypoint": [
      "/kasminit"
    ],
    "user": 1000,
    "environment": {
      "HOME": "/home/kasm-user"
    }
  },
  "exec_config": {},
  "categories": [
    "Productivity"
  ],
  "friendly_name": "KiCad",
  "architecture": [
    "amd64",
    "arm64"
  ],
  "compatibility": [
    {
      "version": "1.16.x",
      "image": "lscr.io/linuxserver/kicad:latest",
      "uncompressed_size_mb": 3690
    }
  ]
}