{
  "description": "Shotcut is a free, open source, cross-platform video editor.",
  "docker_registry": "https://ghcr.io/",
  "image_src": "shotcut.png",
  "run_config": {
    "security_opt": [
      "seccomp=unconfined"
    ],
    "entrypoint": [
      "/kasminit"
    ],
    "user": 1000,
    "environment": {
      "HOME": "/home/kasm-user"
    }
  },
  "exec_config": {},
  "categories": [
    "Multimedia"
  ],
  "friendly_name": "Shotcut",
  "architecture": [
    "amd64"
  ],
  "compatibility": [
    {
      "version": "1.16.x",
      "image": "lscr.io/linuxserver/shotcut:latest",
      "uncompressed_size_mb": 2180
    }
  ]
}