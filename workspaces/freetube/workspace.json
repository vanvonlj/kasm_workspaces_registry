{
  "description": "FreeTube is a feature-rich and user-friendly YouTube client with a focus on privacy.",
  "docker_registry": "https://ghcr.io/",
  "image_src": "freetube.png",
  "run_config": {
    "security_opt": [
      "seccomp=unconfined"
    ],
    "entrypoint": [
      "/kasminit"
    ],
    "user": 1000,
    "environment": {
      "HOME": "/home/kasm-user"
    }
  },
  "exec_config": {},
  "categories": [
    "Multimedia"
  ],
  "friendly_name": "FreeTube",
  "architecture": [
    "amd64",
    "arm64"
  ],
  "compatibility": [
    {
      "version": "1.16.x",
      "image": "lscr.io/linuxserver/freetube:latest",
      "uncompressed_size_mb": 2610
    }
  ]
}